{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["ESNext", "DOM"],
    "types": ["vite/client"],
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["docs/.vitepress/**/*", "docs/**/*.vue"]
}
